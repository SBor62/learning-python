# 1. Суммирование
def sum_range(start, end):
    total = 0
    while start <= end:
        total += start
        start += 1
    return total

start = int(input("Введите начальное число - "))
end = int(input("Введите конечное число - "))

result = sum_range(start, end)
print(f"Сумма чисел от {start} до {end} равна: {result}")
